<h1>Create New Player</h1>

<div class="formparentdiv">
  <form action="/league/{{leagueId}}/team/{{teamId}}/player" method="POST">
    <div>
      <label for="name">Name</label>
      <input type="text" name="name" />
    </div>
    <div>
      <label for="age">Age</label>
      <input type="text" name="age" />
    </div>
    <div>
      <label for="gender">Gender</label>
      <input type="text" name="gender" />
    </div>
    <div>
      <label for="position">Number of Players</label>
      <input type="text" name="position" />
    </div>
    <div>
      <label for="picurl">Division</label>
      <input type="text" name="picurl" />
    </div>
    <div>
      <button type="submit">New Player</button>
    </div>
  </form>
</div>