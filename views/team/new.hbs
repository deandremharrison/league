<h1>Create New Team</h1>

<div class="formparentdiv">
  <form action="/league/{{leagueId}}/team" method="POST">
    <div>
      <label for="name">Name</label>
      <input type="text" name="name" />
    </div>
    <div>
      <label for="age">Logo URL</label>
      <input type="text" name="age" />
    </div>
    <div>
      <label for="gender">Manager</label>
      <input type="text" name="gender" />
    </div>
    <div>
      <label for="position">Number of Players</label>
      <input type="text" name="position" />
    </div>
    <div>
      <label for="picurl">Division</label>
      <input type="text" name="picurl" />
    </div>
    <div>
      <button type="submit">New Team</button>
    </div>
  </form>
</div>